@{
    ViewData["Title"] = "ForgotPasswordConfirmation";
    Layout = "~/Pages/Shared/_LayoutUnauthorized.cshtml";
}



<div class="login-box">
  <div class="card card-outline card-primary">
    <div class="card-header text-center">
      <a asp-action="Login" class="h1"><b>Audit</b>Inspect</a>
    </div>
    <div class="card-body">
      <p class="login-box-msg">Va rugam sa verificati adresa de email pentru a reseta parola.</p>
      
      <p class="mt-3 mb-1">
      
         <input type="button" class="btn btn-primary btn-block" value="Autentificare" onclick="location.href='@Url.Action("Login")'" />
      </p>
    </div>
    <!-- /.login-card-body -->
  </div>
</div>
